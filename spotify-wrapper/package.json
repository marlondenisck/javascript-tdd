{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "spotify-wrapper api",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --out-dir dist",
    "build:watch": "npm run clean && babel src --out-dir dist --watch",
    "build:umd": "npx webpack --output-filename spotify-wrapper.umd.js",
    "build:all": "npm run build && npm run build:umd",
    "test": "jest",
    "test:tdd": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.22.11",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.3",
    "babel-jest": "^29.6.4",
    "babel-loader": "^9.1.3",
    "jest": "^29.6.3",
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
